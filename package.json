{
  "name": "flummox-playground",
  "version": "0.0.1",
  "description": "A Flummox Playground",
  "main": "server/index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "start": "NODE_ENV=production node server",
    "dev": "NODE_ENV=development node server",
    "test": "npm run unit && npm run lint",
    "lint": "./node_modules/.bin/eslint .",
    "unit": "./node_modules/.bin/mocha --compilers js:babel/register --recursive --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebinsua/flummox-playground.git"
  },
  "keywords": [
    "flummox",
    "flux",
    "react",
    "reactjs"
  ],
  "author": "Seb Insua <me@sebinsua.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sebinsua/flummox-playground/issues"
  },
  "homepage": "https://github.com/sebinsua/flummox-playground",
  "engines": {
    "node": ">= 0.10"
  },
  "dependencies": {
    "babel": "^5.1.13",
    "babel-core": "^5.1.13",
    "babel-eslint": "^3.0.1",
    "express": "^4.12.3",
    "express-static": "^1.0.3",
    "flummox": "^4.0.0-alpha",
    "isomorphic-fetch": "^2.0.2",
    "lodash": "^3.7.0",
    "mz": "^1.3.0",
    "nunjucks": "^1.3.4",
    "react": "^0.13.2",
    "react-router": "^0.13.2"
  },
  "devDependencies": {
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.1.13",
    "css-loader": "^0.12.0",
    "eslint": "^0.20.0",
    "eslint-plugin-react": "^2.2.0",
    "node-sass": "^2.1.1",
    "react-hot-loader": "^1.2.5",
    "sass-loader": "^0.6.0",
    "style-loader": "^0.12.1",
    "webpack": "^1.8.10",
    "webpack-dev-server": "^1.8.2"
  }
